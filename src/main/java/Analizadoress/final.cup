sentencias_control::=
    //primera forma--------------
   	if
    instruccion
	| if
    bloque_INIT_END

    //segunda forma--------------
    | if 
    instruccion 
    pametro_els
	| if 
    bloque_INIT_END 
    pametro_els

    //tercera forma--------------
    | if   
    instruccion
    pametros_elseif 
    pametro_els
	| if   
    bloque_INIT_END
    pametros_elseif 
    pametro_els

;
if::=

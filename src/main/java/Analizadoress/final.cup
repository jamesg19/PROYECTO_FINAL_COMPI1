		System.out.println("----HEYYYY :"+cur_token.right);
	}
	public void unrecovered_syntax_error(Symbol s) throws java.lang.Exception{ 
        	System.out.println("Error s√≠ntactico  " +  (s.left)+ " columna "+(s.right)+". palabra " + s.value ); 
    	}
